{
    "_version": "1.8.0",
    "sap.app": {
        "id": "team",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "ManageTeamService": {
                "uri": "/api/manage-team/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "-sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "-id": "ui5template.smartTemplate",
            "-version": "1.40.12"
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "ManageTeamService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect" : true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "TeamsList",
                    "target": "TeamsList"
                },
                {
                    "pattern": "Teams({key}):?query:",
                    "name": "TeamDetails",
                    "target": "TeamDetails"
                },
                {
                    "pattern": "Teams({key}/members({key2}):?query:",
                    "name": "MemberDetails",
                    "target": "MemberDetails"
                }
            ],
            "targets": {
                "TeamsList": {
                    "type": "Component",
                    "id": "TeamsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings" : {
                            "entitySet" : "Teams",
                            "initialLoad": "Enabled",
                            "navigation" : {
                                "Teams" : {
                                    "detail" : {
                                        "route" : "TeamDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "TeamDetails": {
                    "type": "Component",
                    "id": "TeamDetails",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "Teams",
                            "navigation" : {
                                "members" : {
                                    "detail" : {
                                        "route" : "MemberDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "MemberDetails": {
                    "type": "Component",
                    "id": "MemberDetails",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "Members"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}
